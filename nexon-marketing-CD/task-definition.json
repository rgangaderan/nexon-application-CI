{
    "executionRoleArn": "arn:aws:iam::126609036647:role/ecs-execution-role-itf2K",
    "family": "nexon-development-task-definition-wrrhn",
    "networkMode": "awsvpc",
    "containerDefinitions": [{
      "name": "nexon-development-container",
      "image": "126609036647.dkr.ecr.us-east-1.amazonaws.com/nexon-development-gmskr:{{version}}",
      "portMappings": [{
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }],
      "environment": [
        {
          "name": "MYSQL_DBPORT",
          "value": "3306"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:ssm:us-east-1:126609036647:parameter/production/database/password/master",
          "name": "MYSQL_DBPASS"
        },
        {
            "valueFrom": "arn:aws:ssm:us-east-1:126609036647:parameter/production/database/username/master",
            "name": "MYSQL_DBUSER"
        },
        {
            "valueFrom": "arn:aws:ssm:us-east-1:126609036647:parameter/development/database/host/address",
            "name": "MYSQL_DBHOST"
        }

      ],
      "essential": true
    }],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
